# LLM Provider Selection: glm | openai | claude (coming soon)
LLM_PROVIDER=glm

# Z.ai GLM API Key (required when LLM_PROVIDER=glm)
GLM_API_KEY=your_glm_api_key_here

# OpenAI API Key (required when LLM_PROVIDER=openai)
OPENAI_API_KEY=

# Anthropic API Key (required when LLM_PROVIDER=claude)
ANTHROPIC_API_KEY=

# Maximum agent iterations per task (safety limit)
MAX_ITERATIONS=50
